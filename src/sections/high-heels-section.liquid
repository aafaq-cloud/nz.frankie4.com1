<div class="section section-about-us-page-group m-mt-0">

    <div class="section mti-0 mbi-0 m-auto" style="max-width: 2000px;">
        <div class="cell-image cell medium-auto landing-image" 
            style="background-image: url('{{section.settings.image_1 | img_url: '1400x'}}');">
                <div class="text-center landing-text">
                    <h1>{{section.settings.section_main_title}}</h1>
                </div>
        </div>
    </div>

    <div class="section m-mb-0 m-mt-0">
        <div class="grid-container">
            <div class="grid-x align-middle">
                <div class="section-line mti-30 m-mt-30 m-mb-15"></div>
                <div class="text-center text-block1 m-mt-0">
                    <h1 class="mbi-30">{{section.settings.section_title}}</h1>
                    <p>{{section.settings.section_paragraph}}</p>
                </div>
            </div>
        </div>
    </div>

    <div class="section m-mb-0 m-mt-0" style="margin-bottom: 60px">
        <div class="grid-container">
            <div class="grid-x align-middle">
                <div class="cell small-12 medium-6 large-6 column-left wide-screen-only" style="height: 1200px;">
                    <div class="cell-image cell medium-auto image-half-width" 
                        style="background-image: url('{{section.settings.image_2 | img_url: '1400x'}}');
                        margin-bottom: 60px;
                        height:550px;">
                    </div>
                    <div class="cell-image cell medium-auto image-half-width" 
                        style="background-image: url('{{section.settings.image_3 | img_url: '1400x'}}');
                        height:600px;
                        background-size: contain;
                        background-repeat: no-repeat;">
                    </div>
                </div>
                <div class="cell small-12 medium-6 large-6 column-left mobile-screen-only">
                    <div class="cell-image cell medium-auto" 
                        style="background-image: url('{{section.settings.image_3m | img_url: '400x'}}');
                        height:350px;
                        width: 100%;
                        margin-bottom: 20px;
                        background-size: contain;
                        background-repeat: no-repeat;
                        background-position: center">
                    </div>
                </div>
                <div class="cell small-12 medium-6 large-6 column-right m-h800" style="height: 1200px;">
                    {% comment %} <div class="cell-image cell medium-auto image-half-width" 
                        style="background-image: url('{{section.settings.image_4 | img_url: '1400x'}}');
                        height:300px;">
                    </div> {% endcomment %}
                    <div data-autoload-class="ComponentVideo">
                        <video class="lazyload mobile-video" preload="none" autoplay="" loop="" muted=""
                                playsinline controls controlslist="nodownload"
                                width="100%" height="340px"
                                data-src="{{section.settings.video }}">
                        </video>
                    </div>

                    <div class="section-line m-mt-30 m-mb-20"></div>

                    <div class="text-center">
                        <h1>{{section.settings.section_title_2}}</h1>
                        <p>{{section.settings.section_paragraph_2}}</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="section m-mb-0 m-mt-0">
        <div class="grid-container">
            <div class="grid-x align-middle">
                <div class="cell-image cell medium-auto image-full-width wide-screen-only" 
                    style="background-image: url('{{section.settings.image_5 | img_url: '1400x'}}');
                    height: 500px;">
                    <div class="grid-container">
                        <div class="grid-x align-middle">
                            <div class="cell small-7 medium-7 large-7"></div>
                            <div class="cell small-5 medium-5 large-5">
                                <p class="text-right small-text" style="margin: 400px 20px 0 0;">{{section.settings.section_paragraph_5}}</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="cell-image cell medium-auto image-full-width mobile-screen-only" 
                    style="background-image: url('{{section.settings.image_5 | img_url: '1400x'}}');
                    height: 300px; margin-top: 80px">
                    <div class="grid-container">
                        <div class="grid-x align-middle">
                            <div class="cell small-4 medium-4 large-4"></div>
                            <div class="cell small-8 medium-8 large-8">
                                <p class="text-right small-text" style="margin: 170px 10px 0 0;">{{section.settings.section_paragraph_5}}</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="section m-mb-20" style="margin-bottom: 60px">
        <div class="grid-container">
            <div class="grid-x align-middle">

                <div class="cell small-12 medium-6 large-6 column-left">
                    <div class="section-line mti-30 mbi-20 m-mt-0 m-mb-15"></div>
                    <div class="text-center">
                        <img src="{{section.settings.quote_image_1 | img_url: '60x'}}"/>
                        <p class="quote-text">{{section.settings.section_quote}}</p>
                        <p>{{section.settings.section_name}}</p>
                        <img src="{{section.settings.quote_image_2 | img_url: '60x'}}"/>
                    </div>
                    <div class="section-line mti-30 m-mt-10 m-mb-30"></div>
                </div>

                <div class="cell small-12 medium-6 large-6 column-right show-for-medium">
                    <div class="cell-image cell medium-auto image-half-width" 
                        style="background-image: url('{{section.settings.image_6 | img_url: '1400x'}}'); 
                                height: 800px;
                                background-size: contain;
                                background-repeat: no-repeat">
                    </div>
                </div>
                <div class="cell small-12 medium-6 large-6 column-right show-for-small-only">
                    <div class="cell-image cell medium-auto image-half-width" 
                        style="background-image: url('{{section.settings.image_6 | img_url: '1400x'}}'); 
                                height: 600px;
                                background-size: contain;
                                background-repeat: no-repeat">
                    </div>
                </div>
            </div>
        </div>
    </div>




    <div data-autoload-class="WeStandForSection" class="section section-we-stand-for grey-section">

        <div class="grid-container mb-40 m-mb-0">
            <div class="grid-x align-center text-center">
            <div class="cell">
                <h2 class="h1 mb-20 m-mt-40 m-mb-30" style="margin-top: 80px">{{ section.settings.section_title_extra}}</h2>
            </div>
            </div>
        </div>

        <div class="grid-container">

            <div class="tab-titles">
            <div class="glide tab-slider">
                <div class="glide__track" data-glide-el="track">
                <ul class="glide__slides">
                    {% assign tabIndex = 1 %}
                    {% for block in section.blocks %}

                    {% if block.type == 'tab_title' %}
                        <li class="glide__slide">
                            <div class="tab-title-container">
                            <div class="tab-title{% if tabIndex == 1 %} active{% endif %}" data-tab-index="{{ tabIndex }}">
                                <div class="tab-title-container">
                                <img class="tab-title-image" src="{{ block.settings.image | img_url: '500x500', crop: 'center' }}"/>
                                <div class="tab-title-content-container" style="padding: 30px 50px 15px 25px">
                                    <div class="tab-title-content">
                                    <h2>{{ block.settings.title }}</h2>
                                    </div>
                                </div>
                                </div>

                            </div>
                            </div>
                        </li>
                        {% assign tabIndex = tabIndex | plus: 1 %}
                    {% endif %}

                    {% endfor %}
                </ul>
                </div>
            </div>

            </div>

        </div>


        {% comment %}
            Tab Content
        {% endcomment %}
        <div class="grid-container">

            <div class="grid-x">
            <div class="cell large-12">

                <div class="tab-content m-mt-30 m-mb-20 padding-0">
                {% assign tabContentIndex = 1 %}

                {% assign slideTitles = '' %}

                {% for block in section.blocks %}

                    {% if block.type == 'slider_start' %}

                    {% assign slideTitles = '' %}
                    <div class="tab-content-item{% if tabContentIndex == 1 %} active{% endif %}" data-tab-content-index="{{ tabContentIndex }}">
                    <div class="glide slider">
                        <div class="glide__track" data-glide-el="track">
                        <ul class="glide__slides">
                    {% assign tabContentIndex = tabContentIndex | plus: 1 %}

                    {% endif %}

                    {% if block.type == 'slider_end' %}

                        </ul>
                        </div>
                        <div class="glide__bullets wide-screen-only" data-glide-el="controls[nav]">
                        {% assign slides = slideTitles | split: ',' %}
                        {% for bullet in slides %}
                            <button class="glide__bullet" style="background: none;" data-glide-dir="={{ forloop.index0 }}">
                            <span></span>
                            <div class="text-left">{{bullet}}</div>
                            </button>
                        {% endfor %}
                        </div>
                        <div class="glide__bullets mobile-bullet mobile-screen-only" data-glide-el="controls[nav]">
                        {% assign slides = slideTitles | split: ',' %}
                        {% for bullet in slides %}
                            <button class="glide__bullet" style="background: none;" data-glide-dir="={{ forloop.index0 }}">
                            <span></span>
                            {{bullet}}
                            </button>
                        {% endfor %}
                        </div>
                    </div>

                    </div>

                    {% endif %}

                    {% if block.type == 'tab_content' %}

                    <li class="glide__slide">

                        <div class="grid-x grid-margin-x grid-margin-y">
                            <div class="cell medium-12 large-6 tab-content-item-media {{ block.settings.image_placement }}">

                            {% if block.settings.video_url != blank %}
                                {% assign videoId = block.settings.video_url.id %}
                                <div class="video-container">
                                <iframe width="560" height="315" src="https://www.youtube.com/embed/{{ videoId }}" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
                                </div>
                            {% else %}
                                <img src="{{ block.settings.image | img_url: '700x560', crop: 'center' }}"/>
                            {% endif %}

                            </div>
                            <div class="cell medium-12 large-6">
                            <h2 class="h1">{{ block.settings.title }}</h2>
                            <div>{{ block.settings.content }}</div>
                            {% if block.settings.link != blank %}
                            <a class="tab-content-item-link" href="{{ block.settings.link }}" style="text-transform: none;">{{ block.settings.button_text }}</a>
                            {% endif %}
                            </div>
                        </div>

                    </li>

                    {% unless slideTitles == '' %}
                        {% assign slideTitles = slideTitles | append: ',' %}
                    {% endunless %}
                    {% assign slideTitles = slideTitles | append: block.settings.title %}

                    {% endif %}

                {% endfor %}

                </div>
            </div>

            </div>

        </div>
    </div>




    <div class="section m-mb-0">
        <div class="grid-container">
            <div class="grid-x align-middle">
                <div class="cell small-12 medium-6 large-6 column-left wide-screen-only">
                    <div class="cell-image cell medium-auto image-half-width" 
                        style="background-image: url('{{section.settings.image_7 | img_url: '1400x'}}');
                        height: 750px;">
                    </div>
                </div>
                <div class="cell small-12 medium-6 large-6 column-right">
                    <div class="section-line mti-30 mbi-20 m-mt-0 m-mb-15"></div>
                    <div class="text-center">
                        <h1>{{section.settings.section_title_2_1}}</h1>
                        <p>{{section.settings.section_paragraph_2_1}}</p>
                    </div>
                    <div class="section-line mti-30 m-mt-20 m-mb-0"></div>
                    <div class="mobile-screen-only" style="height: 20px;"></div>
                </div>
                <div class="cell small-12 medium-6 large-6 column-left mobile-screen-only">
                    <div class="cell-image cell medium-auto image-half-width" 
                        style="background-image: url('{{section.settings.image_7 | img_url: '1400x'}}'); background-position: bottom">
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="section m-mt-0">
        <div class="grid-container">
            <div class="grid-x align-middle">
                <div class="cell small-12 medium-6 large-6 column-left">
                    <div class="section-line mbi-30 m-mt-30 m-mb-20"></div>
                    <div class="text-center">
                        <h1 class="wide-screen-only" style="font-size: 78px; letter-spacing: -3px;">{{section.settings.section_title_2_2}}</h1>
                        <h1 class="mobile-screen-only">{{section.settings.section_title_2_2}}</h1>
                        <p>{{section.settings.section_paragraph_2_2}}</p>
                    </div>
                </div>
                <div class="cell small-12 medium-6 large-6 column-right">
                    <div class="cell-image cell medium-auto image-half-width m-h400" 
                        style="background-image: url('{{section.settings.image_8 | img_url: '1400x'}}');
                        height: 750px;
                        background-size: contain;
                        background-repeat: no-repeat">
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="section" style="background-color: #F5F5F5;">
        <div class="grid-container">
            <div class="grid-x align-middle pt-0-30" style="padding: 80px 0 60px 0">
                <div class="cell small-12 medium-6 large-6 column-left wide-screen-only">
                    <div data-autoload-class="ComponentVideo">
                        <video class="lazyload" preload="none" autoplay="" loop="" muted=""
                               playsinline controls controlslist="nodownload"
                               width="100%" height="720px"
                               data-src="{{section.settings.video_2 }}">
                        </video>
                    </div>  
                </div>
                <div class="cell small-12 medium-6 large-6 column-right">
                    <div class="text-center">
                        <h1>{{section.settings.section_title_2_3}}</h1>
                        <p>{{section.settings.section_paragraph_2_3}}</p>
                    </div>
                </div>
                <div class="cell small-12 medium-6 large-6 column-left mobile-screen-only">
                    <div data-autoload-class="ComponentVideo">
                        <video class="lazyload" preload="none" autoplay="" loop="" muted=""
                               playsinline controls controlslist="nodownload"
                               width="100%" height="450px"
                               data-src="{{section.settings.video_2 }}">
                        </video>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="section m-mb-0 m-mt-0">
        <div class="grid-container">
            <div class="grid-x align-middle">
                <div class="text-center text-block1 m-mt-0">
                    <p>{{section.settings.section_paragraph_4}}</p>
                    <span><h5><a href="{{section.settings.link_p4}}">{{section.settings.link_p4_name}}</a></h5></span>
                </div>
            </div>
        </div>
    </div>



    <div class="section section-step-inside-slider" data-autoload-class="StepInsideSliderSection">

        <div class="glide slider">
            <div class="glide__arrows mid-screen-show" data-glide-el="controls">
                <button class="glide__arrow glide__arrow--left" data-glide-dir="<">{% render 'icon-chevron-right' %}</button>
                <button class="glide__arrow glide__arrow--right" data-glide-dir=">">{% render 'icon-chevron-right' %}</button>
            </div>
            <div class="glide__track" data-glide-el="track">
                <ul class="glide__slides">
                    {% for block in section.blocks %}
                    {% if block.type == 'slide' %}
                        <li class="glide__slide">
                            <div class="step-inside-item">
                                <a href="{{ block.settings.link }}" target="_blank">
                                    <img class="step-inside-item-image"
                                        src="{{ block.settings.image | img_url: '600x600', crop: 'center' }}"
                                        srcset="
                                        {{ block.settings.image | img_url: '300x300', crop: 'center' }} 300w,
                                        {{ block.settings.image | img_url: '400x400', crop: 'center' }} 400w,
                                        {{ block.settings.image | img_url: '500x500', crop: 'center' }} 500w,
                                        {{ block.settings.image | img_url: '600x600', crop: 'center' }} 600w"
                                        sizes="
                                        (min-width: 600px) 60vw,
                                        (min-width: 850px) 50vw,
                                        (min-width: 1024px) 33.3vw,
                                        (min-width: 1200px) 33.3vw,
                                        (min-width: 1400px) 33.3vw,
                                        80vw"
                                        alt="Image for {{ block.settings.title }}"
                                    />
                                    <div class="step-inside-item-content-container" style="padding: 40px 50px 20px 25px">
                                        <div class="step-inside-item-content">
                                            <div class="step-inside-item-content-subtitle">
                                                <h5>{{ block.settings.subtitle }}</h5>
                                            </div>
                                            <h2>{{ block.settings.title }}</h2>
                                        </div>
                                    </div>
                                </a>

                            </div>

                        </li>
                    {% endif %}
                    {% endfor %}
                </ul>
            </div>
        </div>


    </div>


</div>



